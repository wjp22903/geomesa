.tpInfo
    background-color #f0f0f0
    margin 2px 4px
    border-radius 3px
    padding 1px 2px
    label
        margin-bottom 2px

.tpResult
    margin 2px 4px
    background-color white
    border 2px solid #ddd
    border-radius 4px
    &:hover
        border 2px solid #BBBBBB
    label
        margin-bottom 2px

.tpDataBox
    margin 2px 5px
    padding 4px
    border 1px dotted #ccc
    border-radius 4px
    &.secondaryTpDataBox
        color #666
    &:hover
        border 1px dotted #444
        color #000 !important
