        border-color #444

div.categoryIndent
    padding-left 16px

div.legendIconContainer
    height 16px
    width 16px
    overflow hidden
    display inline-block

span.catGroupButton
    padding 0
    line-height 1
    &:first-of-type
        margin-left 8px
