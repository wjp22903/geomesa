.dcmWizardForm
    width 800px
    max-width 800px
    max-height 600px
    .wizardFormContent
        width 100%
        height 91%
    .predictiveFeatureButton
        width 90%
        max-width 90%
        text-align left
    .btn-group
        width 100%
    td
        border none !important

.wizardFormContent
    .predictiveFeaturesTable
        width inherit
        height 100%
        overflow-y auto
        overflow-x hidden
        display block
        .display-block
            display block
        .btn-group-sm>.btn, .btn-sm
            padding 6px 14px !important