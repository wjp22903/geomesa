.alerts
    .alertListItem
        margin 2px 4px
        background-color white
        border 2px solid white
        border-radius 4px
        padding-left 4px
        &:hover
            border 2px solid #BBBBBB
        &.selected
            border 2px solid #a3d1ff
            background-color #a3d1ff
        &.new
            border 2px solid #61b329
